@model EFCAssets.Models.Office

@{
    ViewData["Title"] = "Delete";
}

<span class="display-4 text-info">Deactivate</span><br />

<text>Are you sure you want to deactivate <span><b>@Model.OfficeName</b></span> office?</text>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OfficeName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeCountry)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OfficeCountry)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Currency)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Currency.CurrencyName)
        </dd class>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="OfficeId" />
        <input type="submit" value="Deactivate" class="btn btn-danger" /> |
        <a asp-action="Index" class="text-info">Back to List</a>
    </form>
</div>
