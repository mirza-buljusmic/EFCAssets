@model EFCAssets.Models.Office

@{
    ViewData["Title"] = "Details";
}

<span class="display-4 text-info">Details</span><span> for @Model.OfficeName office</span>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OfficeName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeCountry)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OfficeCountry)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Currency)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Currency.CurrencyName)
        </dd> 
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeActive)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OfficeActive)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.OfficeId" class="text-info">Edit |</a>
    <a asp-action="Index" class="text-info">Back to List</a>
</div>
