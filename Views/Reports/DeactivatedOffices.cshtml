@model IEnumerable<EFCAssets.Models.Office>

@{
    ViewData["Title"] = "Index";
}


<span>deactivated</span><span style="" class="display-4 text-info">Offices</span>
   <p>
        <a asp-controller="Home" asp-action="Admin" class="text-info">Back to admin page</a>

   </p>
<table class="table kitchensink">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OfficeName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OfficeCountry)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.Currency)
                </th>*@
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.OfficeName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OfficeCountry)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.Currency.CurrencyName)
                    </td>*@
                <td>
                    @*<a asp-action="Edit" asp-route-id="@item.OfficeId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.OfficeId">Details</a> |*@
                    <a asp-action="ActivateOffice" asp-route-id="@item.OfficeId" class="text-info">Activate</a>
                </td>
            </tr>
        }
    </tbody>
</table>
